{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Biere",
  "type": "object",
  "properties": {
    "id": { "type": "integer", "minimum": 1 },
    "name": { "type": "string" },
    "country": { "type": "string" },
    "type": { "type": "string" },
    "alcohol": { "type": "number" },
    "ibu": { "type": "number" },
    "formats": {
      "type": "array",
      "items": { "type": "string" }
    },
    "image": { "type": "string" }
  },
  "required": ["name", "country", "type", "alcohol", "ibu", "formats", "image"]
}
